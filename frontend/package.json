{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "ace-builds": "^1.4.12",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-ace": "^9.4.4",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3",
    "sass": "^1.42.1",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.9.6",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.3.0",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "stylelint": "^13.13.1",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "typescript": "^4.4.3"
  },
  "lint-staged": {
    "**/*.{ts,js,jsx,tsx}": [
      "pretty-quick --staged",
      "eslint --fix"
    ],
    "**/*.{scss,sass}": [
      "stylelint --fix"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix && prettier --write 'src/**/*.{ts,tsx,js,jsx}' --tab-width 2 && stylelint '**/*.{scss,sass}' --fix",
    "lint:ci": "eslint '*/**/*.{js,ts,tsx}' --color && yarn prettier --check 'src/**/*.{ts,tsx,js,jsx}' --tab-width 2 && stylelint '**/*.{scss,sass}' --color"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
