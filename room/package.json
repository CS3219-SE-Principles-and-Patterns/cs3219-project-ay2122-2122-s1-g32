{
  "name": "room-management",
  "version": "0.0.1",
  "description": "Room Management service for Code2Gather",
  "private": true,
  "devDependencies": {
    "lint-staged": "^11.2.6"
  },
  "scripts": {
    "db:seed": "go run ./src/infra/db/db_script/main/db_question_seed.go",
    "lint": "gofmt -w .",
    "postinstall": "go mod vendor",
    "start": "nodemon --exec go run ./src/main.go --signal SIGTERM"
  },
  "lint-staged": {
    "**/*.go": [
      "gofmt -w ."
    ]
  },
  "dependencies": {
    "nodemon": "^2.0.14"
  }
}
