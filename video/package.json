{
  "name": "video",
  "version": "0.0.1",
  "description": "Video call service for Code2Gather",
  "private": true,
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-sort-requires": "^2.1.0",
    "is-ci": "^3.0.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1"
  },
  "scripts": {
    "lint": "eslint . --fix --ext .js && prettier --write \"**/*.js\" --tab-width 2",
    "lint:ci": "eslint . --ext .js && prettier --check \"**/*.js\" --tab-width 2",
    "start": "nodemon index.js"
  },
  "lint-staged": {
    "**/*.js": [
      "pretty-quick --staged",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "agora-access-token": "^2.0.4",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.14"
  }
}
